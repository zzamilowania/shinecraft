          "_name": "apeople4",
          "_sourceTheme": "expertm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"people4 expertm5\" group=\"People\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n        <select title=\"Align\" name=\"contentAlign\">\n            <option value=\"justify-content-start\">Left</option>\n            <option value=\"justify-content-center\" selected>Center</option>\n            <option value=\"justify-content-end\">Right</option>\n        </select>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showMainTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showMainSubtitle\" checked>\n        <select title=\"Cards\" name=\"cardsAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\" selected>6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\">8</option>\n        </select>\n        <header>Card</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Border\" name=\"showBorder\" checked condition=\"showTitle\">\n        <input type=\"color\" title=\"Border Color\" name=\"borderColor\" value=\"#16181c\" condition=\"showBorder && showTitle\">\n        <header>Card Style</header>\n        <input type=\"color\" title=\"Card BG 1\" name=\"cardBg1\" value=\"#1e2024\">\n        <input type=\"color\" title=\"Card BG 2\" name=\"cardBg2\" value=\"#23272b\">\n        <input type=\"checkbox\" name=\"cardShadow\" title=\"Card Shadow\" checked>\n        <input type=\"color\" title=\"Shadow Color 1\" name=\"shadowColor1\" value=\"#191b1f\" condition=\"cardShadow\">\n        <input type=\"color\" title=\"Shadow Color 2\" name=\"shadowColor2\" value=\"#23272B\" condition=\"cardShadow\">\n        <header>Card Hover</header>\n        <input type=\"color\" title=\"Card BG 1\" name=\"cardHoverBg1\" value=\"#212428\">\n        <input type=\"color\" title=\"Card BG 2\" name=\"cardHoverBg2\" value=\"#16181c\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/expertm5/components/_images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#212428\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/expertm5/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#212428\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12\" mbr-if=\"showMainTitle || showMainSubtitle\">\n                <div class=\"mbr-section-head\">\n                    <h4 class=\"mbr-section-title mbr-fonts-style align-center mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showMainTitle\" data-app-selector=\".mbr-section-title\"><b>\n                        OFERUJEMY:</b></h4>\n                    <h5 class=\"mbr-section-subtitle mbr-fonts-style align-center mb-0\" mbr-theme-style=\"display-2\" mbr-if=\"showMainSubtitle\" data-app-selector=\".mbr-section-subtitle\"><b>PIELĘGNACJA I OCHRONA</b></h5>\n                </div>\n            </div>\n        </div>\n        <div class=\"row card-row {{contentAlign}}\">\n            <div class=\"card col-12 col-sm-6 col-md-4 col-lg-3\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-bg\"></div>\n                    <div class=\"card-box\">\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/a-170x171.png\" alt=\"1\">\n                        </div>\n                        <div class=\"text-item\" mbr-if=\"showTitle\">\n                            <h5 class=\"card-title align-center mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\"><b>Korekta lakieru / polerowanie</b> <br>– usuwamy zarysowania, przywracamy głębię koloru i gładkość powierzchni.</h5>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"card col-12 col-sm-6 col-md-4 col-lg-3\" mbr-if=\"cardsAmount > 1\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-bg\"></div>\n                    <div class=\"card-box\">\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/b-2-200x170.png\" alt=\"2\">\n                        </div>\n                        <div class=\"text-item\" mbr-if=\"showTitle\">\n                            <h5 class=\"card-title align-center mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">\n                                \n                            <div><span style=\"font-size: 1.2rem; background-color: initial;\"><b>Powłoki ceramiczne i woski</b> – trwała ochrona przed brudem i czynnikami zewnętrznymi.</span><br></div><div>\n</div><div>\n</div><div>\n</div><div>\n</div></h5>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"card col-12 col-sm-6 col-md-4 col-lg-3\" mbr-if=\"cardsAmount > 2\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-bg\"></div>\n                    <div class=\"card-box\">\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/c-1-170x170.png\" alt=\"3\">\n                        </div>\n                        <div class=\"text-item\" mbr-if=\"showTitle\">\n                            <h5 class=\"card-title align-center mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\"><b>Polerowanie lamp</b> <br>– poprawa przejrzystości i skuteczności oświetlenia.</h5>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"card col-12 col-sm-6 col-md-4 col-lg-3\" mbr-if=\"cardsAmount > 3\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-bg\"></div>\n                    <div class=\"card-box\">\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/d-170x170.png\" alt=\"4\">\n                        </div>\n                        <div class=\"text-item\" mbr-if=\"showTitle\">\n                            <h5 class=\"card-title align-center mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\"><b>Czyszczenie i zabezpieczenie felg, plastików zewnętrznych</b><br>– pełna pielęgnacja detali.</h5>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"card col-12 col-sm-6 col-md-4 col-lg-3\" mbr-if=\"cardsAmount > 4\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-bg\"></div>\n                    <div class=\"card-box\">\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/e-170x128.png\" alt=\"5\">\n                        </div>\n                        <div class=\"text-item\" mbr-if=\"showTitle\">\n                            <h5 class=\"card-title align-center mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\"><b>Czyszczenie wnętrza i pranie tapicerki </b><br>– dogłębne czyszczenie, usuwanie zapachów i bakterii.</h5>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"card col-12 col-sm-6 col-md-4 col-lg-3\" mbr-if=\"cardsAmount > 5\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-bg\"></div>\n                    <div class=\"card-box\">\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/f-170x172.png\" alt=\"6\">\n                        </div>\n                        <div class=\"text-item\" mbr-if=\"showTitle\">\n                            <h5 class=\"card-title align-center mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\"><b>Czyszczenie i impregnacja skóry </b><br>– elastyczność, czystość i ochrona przed pękaniem.</h5>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"card col-12 col-sm-6 col-md-4 col-lg-3\" mbr-if=\"cardsAmount > 6\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-bg\"></div>\n                    <div class=\"card-box\">\n                        <div class=\"item-img\">\n                            <img src=\"@ADDONS_PATH@/expertm5/components/_images/clients/1.png\" alt=\"Mobirise Website Builder\">\n                        </div>\n                        <div class=\"text-item\" mbr-if=\"showTitle\">\n                            <h5 class=\"card-title align-center mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">\n                                <a href=\"#\" class=\"text-white\">Fringilla urna</a>\n                            </h5>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"card col-12 col-sm-6 col-md-4 col-lg-3\" mbr-if=\"cardsAmount > 7\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-bg\"></div>\n                    <div class=\"card-box\">\n                        <div class=\"item-img\">\n                            <img src=\"@ADDONS_PATH@/expertm5/components/_images/clients/2.png\" alt=\"Mobirise Website Builder\">\n                        </div>\n                        <div class=\"text-item\" mbr-if=\"showTitle\">\n                            <h5 class=\"card-title align-center mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">\n                                <a href=\"#\" class=\"text-white\">Fringilla urna</a>\n                            </h5>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
